#!./exe

let C0 '\e[0m'      # Reset
let C3 '\e[0;33m'   # Yellow
let C8 '\e[0;90m'   # Gray

let LOGO """
        ${C8}#####${C0}
       ${C8}#######${C0}
       ${C8}##${C0}0${C8}#${C0}0${C8}##${C0}
       ${C8}#${C3}#####${C8}#${C0}
     ${C8}##${C0}##${C3}###${C0}##${C8}##${C0}
    ${C8}#${C0}##########${C8}##${C0}
   ${C8}#${C0}############${C8}##${C0}
   ${C8}#${C0}###########${C8}####${C0}
  ${C3}##${C8}#${C0}###########${C8}##${C3}#${C0}
${C3}######${C8}#${C0}#######${C8}#${C3}######${C0}
${C3}#######${C8}#${C0}#####${C8}#${C3}#######${C0}
  ${C3}#####${C8}#######${C3}#####${C0}
"""

echo -e "$LOGO"
